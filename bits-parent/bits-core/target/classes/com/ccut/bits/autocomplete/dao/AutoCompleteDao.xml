<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD iBatis Mapper 3.0 //EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ccut.bits.autocomplete.dao.AutoCompleteDao">

    <select id="getRoleAutoCompleteList" resultType="AutoComplete">
        SELECT id,role_name name FROM sys_role
    </select>

    <select id="getThemeAutoCompleteList" resultType="AutoComplete">
        SELECT id, theme_name NAME FROM theme_type
    </select>


</mapper>